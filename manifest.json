{
  "manifest_version": 3,
  "name": "JustErikDemon",
  "version": "1.0.0",
  "description": "BobRossRocks - Custom Roblox Sidebar Button Extension",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["https://www.roblox.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
