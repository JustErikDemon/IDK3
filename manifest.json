{
    "manifest_version": 3,
    "name": "Roblox Sidebar Enhancer",
    "version": "1.0",
    "description": "Adds JustErikDemon Rewards button to the Roblox sidebar.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.roblox.com/*"],
            "js": ["content.js"],
            "css": ["styles.css"]
        }
    ],
  
    }
}
